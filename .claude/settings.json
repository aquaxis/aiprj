{
  "env": {
    "API_TIMEOUT_MS": "1800000",
    "BASH_DEFAULT_TIMEOUT_MS": "1800000"
  },
  "permissions": {
    "allow": [
      "Bash(grep:*)",
      "ls",
      "cat",
      "code edit"
    ],
    "deny": [
      "Bash(rm -rf /)",
      "Bash(rm -rf ~)",
      "Bash(rm -rf ~/*)",
      "Bash(rm -rf /*)"
    ]
  },
  "autoApproveEdits": true
}
